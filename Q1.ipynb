    "## Q1\n",
    "def sol(n):\n",
    "    if (n >= 0 & n < 10**9):\n",
    "        if (n%3 != 0): notes = (n//3)*2+1\n",
    "        else: notes = (n//3)*2        \n",
    "    return notes\n",
    "           \n",
    "       \n"
   ]
